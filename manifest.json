{
  "manifest_version": 2,
  "name": "Hide Github Stats",
  "version": "1.0",
  "description": "Hide github stats.",
  "permissions": [
    "https://github.com"
  ],
  "content_scripts": [
    {
      "matches": ["*://github.com/*"],
      "css": ["content.css"]
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
